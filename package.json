{
  "name": "jours-feries-france-js",
  "version": "1.0.0",
  "description": "jours fériés en France",
  "keywords": [
    "node",
    "typescript",
    "ferie",
    "ferié",
    "france"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-url"
  },
  "author": "christophe bellec",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "yarn ts-node src/index.ts",
    "dev": "nodemon --watch '*.ts' --exec 'ts-node' ./src/index.ts",
    "eslint:fix": "eslint --fix",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "prepare": "yarn build",
    "prepublishOnly": "yarn prettier && yarn lint",
    "lint": "eslint \"{**/*,*}.{js,ts}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts}\""
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "date-fns": "^3.4.0"
  }
}
